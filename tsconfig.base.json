{
	"compilerOptions": {
		/* 环境 */
		"lib": [
			"ESNext"
		],
		/* 静态分析 */
		"strict": true,
		/* 兼容 */
		"moduleResolution": "Node",
		"esModuleInterop": true,/* __esModule */
		"allowSyntheticDefaultImports": true,/* 将commonjs 的 exports挂到default上 */
		/* 输出 */
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true,
		/* 子模块使用references */
		"composite": true
	},
	"include": []
}